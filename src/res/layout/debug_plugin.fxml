<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="712.0" prefWidth="821.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="application.controller.DebugPluginController">
    <children><!--text="C:\Users\MyPC\IdeaProjects\TestDemo"-->
        <TextField fx:id="tf_projectPath" layoutX="40.0" layoutY="59.0" prefHeight="35.0"
                   prefWidth="451.0" promptText="填入Cordova项目的绝对路径"/>
        <TextField fx:id="tf_pluginPath" layoutX="40.0" layoutY="129.0" prefHeight="35.0"
                   prefWidth="451.0" promptText="填入插件的绝对路径"/>
        <Label layoutX="40.0" layoutY="37.0" prefHeight="22.0" prefWidth="97.0"
               text="Cordova项目路径"/><!--text="C:\Users\MyPC\IdeaProjects\TestPlugin"-->
        <Label layoutX="40.0" layoutY="105.0" prefHeight="22.0" prefWidth="76.0" text="插件目录路径"/>
        <TextArea fx:id="displayLog" editable="false" layoutX="37.0" layoutY="201.0"
                  prefHeight="400" prefWidth="550.0" promptText="执行结果显示区域:"/>
        <CheckBox fx:id="lockproject" layoutX="510.0" layoutY="69.0" mnemonicParsing="false"
                  onMouseClicked="#lockProjectPathInput" text="是否锁定"/>
        <CheckBox fx:id="lockplugin" layoutX="510.0" layoutY="139.0" mnemonicParsing="false"
                  onMouseClicked="#lockPluginPathInput" text="是否锁定"/>
        <Button layoutX="655.0" layoutY="282.0" mnemonicParsing="false"
                onMouseClicked="#addPlugin2project" prefHeight="35.0" prefWidth="142.0"
                text="将插件添加到项目"/>
        <Button layoutX="655.0" layoutY="363.0" mnemonicParsing="false"
                onMouseClicked="#buildProject" prefHeight="35.0" prefWidth="142.0"
                text="编译Cordova项目"/>
        <Button layoutX="652.0" layoutY="201.0" mnemonicParsing="false"
                onMouseClicked="#addPlatform" prefHeight="35.0" prefWidth="142.0"
                text="给Cordova添加平台"/>
        <ProgressIndicator fx:id="mProgressIndicator" layoutX="283.0" layoutY="354.0"
                           visible="false"/>
        <Button layoutX="652.0" layoutY="444.0" mnemonicParsing="false" prefHeight="35.0"
                prefWidth="142.0" text="插件列表显示" onMouseClicked="#showPluginList"/>

        <Button layoutX="652.0" layoutY="540.0" mnemonicParsing="false" prefHeight="35.0"
                prefWidth="142.0" text="重新添加插件" onMouseClicked="#replugin"/>
    </children>
</AnchorPane>
